	author = {Walker, G.},
	year = {2014},
	note = {Publisher: Water Alternatives Association},
	keywords = {England, England and Wales, Meta-governance, United Kingdom, Wales, Water governance, Water privatisation, Water scarcity, governance approach, innovation, privatization, water resource, water supply},
	pages = {388--413}
}

@article{raso_short-term_2014,
	title = {Short-term optimal operation of water systems using ensemble forecasts},
	volume = {71},
	issn = {03091708 (ISSN)},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84904419394&doi=10.1016%2fj.advwatres.2014.06.009&partnerID=40&md5=90804574ca60b48af2ad4849a7f9b9e5},
	doi = {10.1016/j.advwatres.2014.06.009},
	abstract = {Short-term water system operation can be realized using Model Predictive Control (MPC). MPC is a method for operational management of complex dynamic systems. Applied to open water systems, MPC provides integrated, optimal, and proactive management, when forecasts are available. Notwithstanding these properties, if forecast uncertainty is not properly taken into account, the system performance can critically deteriorate.Ensemble forecast is a way to represent short-term forecast uncertainty. An ensemble forecast is a set of possible future trajectories of a meteorological or hydrological system. The growing ensemble forecasts' availability and accuracy raises the question on how to use them for operational management.The theoretical innovation presented here is the use of ensemble forecasts for optimal operation. Specifically, we introduce a tree based approach. We called the new method Tree-Based Model Predictive Control (TB-MPC). In TB-MPC, a tree is used to set up a Multistage Stochastic Programming, which finds a different optimal strategy for each branch and enhances the adaptivity to forecast uncertainty. Adaptivity reduces the sensitivity to wrong forecasts and improves the operational performance.TB-MPC is applied to the operational management of Salto Grande reservoir, located at the border between Argentina and Uruguay, and compared to other methods. © 2014 Elsevier Ltd.},
	language = {English},
	journal = {Advances in Water Resources},
	author = {Raso, L. and Schwanenberg, D. and van de Giesen, N.C. and van Overloop, P.J.},
	year = {2014},
	note = {Publisher: Elsevier Ltd},
	keywords = {Argentina, Complex dynamic systems, Ensemble forecast, Ensemble forecasts, Forecast uncertainty, Forecasting, Model Predictive Control, Model predictive control, Multi-stage stochastic programming, Multistage Stochastic Programming, Operational management, Operational water management, Paraguay, Predictive control systems, Reservoir management, Robustness, Robustness (control systems), Salto Grande Reservoir, Short term, Short term reservoir management, Short-term optimal operation, Stochastic programming, Theoretical innovations, Water management, Waterworks, ensemble forecasting, performance assessment, planning method, prediction, reservoir, stochasticity, strategic approach, uncertainty analysis, water management, water resource},
	pages = {200--208}
}

@article{lynch_policy_2014,
