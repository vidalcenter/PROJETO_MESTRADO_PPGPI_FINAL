@article{sukanta_cultivo_nodate,
	title = {Cultivo e opções de estabelecimento de culturas para melhorar a produtividade, rentabilidade e eficiência no uso de recursos dos sistemas arroz-rabi das terras baixas costeiras afetadas por sal do leste da Índia},
	doi = {https://doi.org/10.1016/j.fcr.2019.03.016},
	abstract = {Os agricultores nas áreas costeiras afetadas por sal do leste da Índia enfrentam desafios de inundação, salinidade, escassez de água de irrigação e chuvas insuficientes que afetam a produção agrícola e a sustentabilidade ambiental. Este estudo avaliou três métodos de estabelecimento de arroz [arroz seco semeado (DSR), arroz transplantado não ensopado (não PTR) e arroz transplantado (PTR)] durante a estação chuvosa / kharif e três práticas de preparo do solo [semeadura direta ( ZT), preparo convencional (CT) e semeadura em canteiros (RBS)] para culturas de seca / rabi (milho e colza) em um experimento replicado. O local experimental foi na área de planície costeira de West Bengal, com efeito de sequeiro e de água salgada, e o arroz foi cultivado em três estações húmidas sucessivas (2013–16), com cada cultura de arroz seguida de milho ou colza na estação seca. O arroz transplantado foi afetado negativamente pela submersão a cada ano devido à forte chuva após o transplante, enquanto o DSR foi menos afetado, pois as plantas eram mais altas no momento em que as fortes chuvas começaram. Os rendimentos de grãos de arroz foram semelhantes (4,5–5,1 t ha −1 ), no entanto, entre os três métodos de estabelecimento de arroz, e, além disso, os rendimentos de arroz não foram afetados pelas práticas de plantio direto da estação da raiva. Os retornos líquidos e a relação custo-benefício (BCR) foram maiores no DSR devido aos custos reduzidos de preparação da terra. A colza cultivada após arroz não envasado (DSR e não-PTR) deu um rendimento 25-44\% maior do que a colza cultivada após PTR, e os rendimentos de milho foram 8–13\% maiores quando cultivados após DSR ou não-PTR do que após PTR . DSR e Non-PTR também tiveram efeitos positivos na qualidade do solo: a densidade do solo foi menor após a colheita do arroz no terceiro ano e a salinidade do solo diminuiu durante cada estação seca (dezembro a abril) logo após a cultura do arroz. A produção de milho foi mais alta na ERB (5,8 t ha −1 ), seguida pela CT (4,1 t ha −1 ) e ZT (3,6 t ha −1 ), enquanto a colza produziu a maior produção sob ZT (0,86 t ha −1 ) -à-vis RBS e CT (0,6 t ha- 1 ). Os retornos líquidos para o milho foram maiores com as safras na RBS e na colza com ZT. O milho proporcionou um retorno econômico maior do que a colza (US \$ 301-405 ha- 1 em milho; US \$ 5-113 ha- 1 em colza), mas exigiu 25 cm a mais de água de irrigação e 3 GJ ha- 1 a mais de energia. cultivo. Este estudo indica que o arroz-milho poderia ser mais lucrativo do que a colza para as áreas costeiras afetadas pelo sal na Índia, e que, para melhorar a produtividade e a lucratividade nessa rotação, o arroz deveria ser estabelecido como DSR ou Não-PTR seguido de milho na RBS nas regiões costeiras.},
	author = {Sukanta, K. Sarangi and Sudhanshu, Singh and Virender, Kumar and Ashish, K. Srivastava and Parbodh, C. Sharma and David, E. Johnson}
}

