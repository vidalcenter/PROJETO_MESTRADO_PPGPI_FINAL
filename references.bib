	editor = {Singh, Shashi Bala and Prabhakar, Nanduri R. and Pentyala, Srinivas N},
	year = {2014},
	doi = {10.1007/978-81-322-1928-6_2},
	keywords = {Frugal Innovation, High Yielding Variety, Luffa Acutangula, Onion Variety, Ridge Gourd},
	pages = {11--16}
}

