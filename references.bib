@article{costa_interseccao_2011,
	title = {Intersecção entre {Inovação} e {Propriedade} {Intelectual}: uma {Análise} {Bibliométrica}},
	issn = {2177-3866},
	abstract = {The subjects Innovation and Intellectual Properties (IP) have shown increasing importance for researches in the Management area. Many studies have analyzed the interactions between these fields, and its relevance for the competitiveness of organizations. To better understand the evolution of a scientific subject matter, it is acknowledged important to survey the bibliographic production about it, because it allows the identification of eventual gaps and not solved issues, and can be used as a base to purpose new researches. In this context, the authors of this paper present a quantitative analysis of the scientific production about Innovation and IP, between 2000 and 2010, tracing a panorama of the state of the art at the intersection of the two fields. Two samples were extracted from ISI and Scopus databases, containing respectively 3,142 and 2,876 published titles, at the intersection of these themes. Featured articles are cited in the samples, in addition to the journals and authors more productive and more referenced. We believe this study contributes to better understand the knowledge published about Innovation and IP, serving as inspiration for researchers seeking to develop new studies, and helping them to create a good strategy for their literature review.},
	language = {pt},
	number = {XIV Edição},
	author = {Costa, Renato Machado and Froehner, Juliano},
	year = {2011},
	pages = {18}
}

