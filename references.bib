@article{acheampong_sustainable_2016,
	title = {Sustainable {Urban} {Water} {System} {Transitions} {Through} {Management} {Reforms} in {Ghana}},
	volume = {30},
	issn = {09204741 (ISSN)},
	doi = {10.1007/s11269-016-1256-3},
	abstract = {Despite decades of water reforms, Ghana’s struggle to achieve sustainable urban water system is deepened by complex interactions of multi-layered political, socio-economic and managerial characteristics, leaving a rationing system of water supply in major cities like Accra. Using a multi-level perspective framework, the paper examines the dynamics of urban water system transition through management reforms. The study showed how external pressure at the landscape level influenced policy direction within urban water regime through the implementation of neo-liberal economic policies, paving way for resistance and grassroots innovation at the niche level. The implementation of such policies in the reform process did little to help achieve the desired sustainable urban water system goals. The paper suggests a blend of public and private financing with support for grassroots to improve urban water system management. However, subsequent urban water policy reforms must be informed by knowledge of social, economic, and political realities rather than imported generic “best policies and practices” that often conflict with local realities. © 2016, Springer Science+Business Media Dordrecht.},
	language = {English},
	number = {5},
	journal = {Water Resources Management},
	author = {Acheampong, E.N. and Swilling, M. and Urama, K.},
	year = {2016},
	note = {Publisher: Springer Netherlands},
	keywords = {Economic policies, Economics, External pressures, Ghana, Management, Multi-level perspective, Multilevels, Policies, Private financing, Public policy, Reforms, Socio-economics, Transition, Urban water system, Urban water systems, Water management, Water supply, Waterworks, economic policy, grassroots level, policy making, policy reform, urban drainage, water management, water resource},
	pages = {1835--1849}
}

