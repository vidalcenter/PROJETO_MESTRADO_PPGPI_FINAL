	pages = {139--146}
}

@article{gutierrez-malaxechebarria_informal_2013,
	title = {Informal irrigation in the colombian andes: {Local} practices, national agendas, and options for innovation},
	volume = {33},
	issn = {02764741 (ISSN)},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84883706364&doi=10.1659%2fMRD-JOURNAL-D-12-00116.1&partnerID=40&md5=f55022f3e3956e02182aacb490ebb225},
	doi = {10.1659/MRD-JOURNAL-D-12-00116.1},
	abstract = {Knowledge about the extent of irrigated land in Colombia is scarce because most irrigation is done informally and therefore not registered. Informal irrigation in Colombia has not been investigated, its national presence has not been identified, and its characteristics are still unknown. The present article aims to help fill this gap. Because of topographical and agricultural patterns, Andean informal irrigation is impossible to identify or characterize using standard methods. To provide information on the scale of informal irrigation systems in Colombia, their type, the users they serve, and the type of water sources they use, a large-scale partial inventory was made combining statistical analysis, questionnaires, interviews, and field trips. The findings indicate that informal irrigation is widespread in the country's mountainous zones, in a great diversity of environments, characterized by an equally great diversity of local uses and water-use strategies. This diversity has resulted from the coexistence of very different community and state institutions, posing challenges for planning and organizing water-resource management more efficiently and sustainably for producers, the government, and other stakeholders.},
	language = {English},
	number = {3},
	journal = {Mountain Research and Development},
	author = {Gutierrez-Malaxechebarria, A.-M.},
	year = {2013},
	keywords = {Andes, Colombia, Informal irrigation, agricultural practice, household farming, irrigation, mountain region, production strategies, sustainability, topography, water management, water resource, water use efficiency},
	pages = {260--268}
}

@article{li_growth_2019,
	title = {Growth of wave height with retreating ice cover in the {Arctic}},
