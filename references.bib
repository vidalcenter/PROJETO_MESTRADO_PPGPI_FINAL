	year = {2011},
	note = {Publisher: WITPress},
	keywords = {Ecosystems services, Wfd-economics, conservation management, ecosystem service, environmental policy, innovation, land use, participatory approach, policy implementation, resource allocation, stakeholder, sustainable development, water management, water planning},
	pages = {75--86}
