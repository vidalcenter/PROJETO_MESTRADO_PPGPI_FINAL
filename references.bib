	year = {2016},
	note = {Publisher: Chinese Society of Agricultural Engineering},
	keywords = {Disaster prevention, Disasters, Economic and social effects, Economic development, Economics, Environmental pollutions, Evaluation index system, Index system, Index systems, Land reclamation, Modernization, Pollution, Pollution control, Potable water, Quality control, Rural areas, Rural water conservancies, Statistical software, Sustainable utilization, Water, Water conservancy, Water management, Water pollution, Water pollution control, Water quality, Water resources, Water supply},
	pages = {171--178}
}

