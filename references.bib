@article{sepulveda_business_2011,
	title = {Business in climate or climate in business?},
	volume = {22},
	issn = {14777835 (ISSN)},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-80052634994&doi=10.1108%2f14777831111159752&partnerID=40&md5=15d1735e4f88490818de2fced6af6cd2},
	doi = {10.1108/14777831111159752},
	abstract = {Purpose: The aim of this paper is to present a new methodological approach to help companies improve their decision-taking capacity regarding corporate social responsibility (CSR) in the context of climate change (CC). Design/methodology/approach: The study was conducted by combining a wide variety of sources including articles and research reports. Taking into account the complexity of adapting in a context of climate change, the paper developed a simplified conceptual model that integrates multi-criteria analysis. A vulnerability matrix was generated as a combination of adaptive capacity, exposure and sensitivity and how to measure the magnitude, persistence, uncertainty, temporality, degree of importance in combination with the impacts. Findings: The first results reveal that activities related to water resources or natural ecosystems are more important than those related to energy or tourism sectors, and that there is a relationship between opportunities and adaptation needs. Practical implications: The paper documents the inverse relationship between adaptation measures and opportunities in a study that may be a starting-point for further research into empirical observations of sector vulnerability and the impact and integration of the factor of resilience. Originality/value: The paper provides a common language to use in the business world for concepts related to climate change. It provides a methodological approach for finding solutions to the problem of resource optimisation in companies and helps to find new business opportunities for sectors. © Emerald Group Publishing Limited.},
	language = {English},
	number = {5},
	journal = {Management of Environmental Quality: An International Journal},
	author = {Sepúlveda, J. and Mendizabal, M.},
	year = {2011},
	note = {Publisher: Emerald Group Publishing Ltd.},
	keywords = {Adaptability, Climate change, Impact, Innovation, Opportunity, Prioritisation, Vulnerability},
	pages = {632--651}
}

@article{fritz_factor_2011,
	title = {The {Factor} {Four} concept in {Eco}-{Hydrology} - {Is} it applicable?},
	volume = {11},
	issn = {16423593 (ISSN)},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84859473729&doi=10.2478%2fv10104-011-0052-1&partnerID=40&md5=ed3f2df707c1492e4b4b4fca0e0ac486},
	doi = {10.2478/v10104-011-0052-1},
	abstract = {The problems of shortages of water and its quantity at a global scale call for urgent attention. In the last 20 years the focus of environmental policy shifted from economical to biological dimension. The Factor Four concept of von Weizäcker was originally conceived for solving economic issues of overexploited natural resources. Implementation of an array of technological innovations is leading to sustainability, however this is not enough. Now the concept could be applied to ecosystems related issues, like water resources. Services rendered by ecosystems are linked to economic activity of humans. Doubling efforts to "protect" water resources will lead to twofold increase in its "productivity". Proper land management improves water resources, which enhances biodiversity and finally protects environment. Some examples of water resources management in the light of ecohydrology is proposed. The conflict between protection versus use of ecosystems is also discussed. Initiative on the Economics of Ecosystems and Biodiversity (TEEB) is proposed as a possible approach.},
