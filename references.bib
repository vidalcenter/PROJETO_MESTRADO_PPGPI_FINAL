	keywords = {Biofilters, Biofiltration, Biogeochemistry, Biological materials, Biological oxidations, Chemicals removal (water treatment), Efficiency, Hydraulic retention time, Organic compounds, Organic matter removal, Organics removal, Pollution, Pollutted river water, Potable water, Pre-Treatment, Pre-treatment, Quartz, Raw water, Removal, River pollution, River water, Rivers, Sand, Total suspended solids, Water, Water pollution, Water resources, Water supply, Water treatment},
	pages = {203--209}
}

@article{massara_mini_2017,
	title = {A {Mini} {Review} of the {Techno}-environmental {Sustainability} of {Biological} {Processes} for the {Treatment} of {High} {Organic} {Content} {Industrial} {Wastewater} {Streams}},
	volume = {8},
	issn = {18772641 (ISSN)},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85025067607&doi=10.1007%2fs12649-017-0022-y&partnerID=40&md5=8330f6bc7473307b4f5db9de0031b881},
	doi = {10.1007/s12649-017-0022-y},
	abstract = {Abstract: Industrial wastewater contains complex and slowly biodegradable compounds often ineffectively treated by conventional activated sludge (CAS) systems. Alternatively, advanced anaerobic technologies are implemented. The current study reviews different potential anaerobic schemes, factors influencing their final performance and optimum combinations of operational/design parameters. Anaerobic membrane bioreactors, upflow anaerobic sludge blanket reactors, expanded granular sludge beds, anaerobic hybrid reactors and inverse fluidized bed reactors are discussed. Their major advantages include: low energy requirements, energy recovery through biogas generation and high organic load removal. pH = 7, operation in a mesophilic environment and a hydraulic retention time long enough to enable anaerobic digestion in economically accepted reactor volumes are conditions that optimize the performance of anaerobic configurations. The evaluation additionally considers environmental aspects. The life cycle assessment of anaerobic industrial wastewater treatment reveals its positive environmental effect in terms of greenhouse gases emissions. Methane (a greenhouse gas) primarily contained in the biogas, despite being produced during anaerobic digestion, is utilized for energy production (heating, electricity) instead of being emitted to the atmosphere. Finally, anaerobic wastewater treatment is analyzed as part of the European Commission Innovation Deal that aims at converting conventional wastewater treatment plants to water resource recovery facilities able to combine sustainable wastewater treatment and water reuse. Graphical Abstract: [Figure not available: see fulltext.]. © 2017, Springer Science+Business Media B.V.},
	language = {English},
	number = {5},
	journal = {Waste and Biomass Valorization},
	author = {Massara, T.M. and Komesli, O.T. and Sozudogru, O. and Komesli, S. and Katsou, E.},
	year = {2017},
	note = {Publisher: Springer Netherlands},
	keywords = {Activated sludge process, Advanced anaerobic technologies, Anaerobic digestion, Anaerobic waste water treatment, Biogas, Biogas production, Biological water treatment, Bioreactors, Circular economy, Fluid catalytic cracking, Fluidized beds, Greenhouse effect, Greenhouse gases, Industrial economics, Industrial emissions, Industrial wastewater, Industrial wastewater treatment, Industrial wastewaters, Industrial water treatment, Life Cycle Assessment (LCA), Life cycle, Life cycle assessment, Sustainable development, Sustainable wastewater treatments, Up-flow anaerobic sludge blanket reactors, Wastewater reclamation, Wastewater treatment, Water conservation},
