@article{gomes_educacao_2018,
	title = {Educação empreendedora no ensino profissional: desafios e experiências numa instituição de ensino},
	volume = {1},
	copyright = {Direitos autorais 2018 HOLOS},
	issn = {1807-1600},
	shorttitle = {{EDUCAÇÃO} {EMPREENDEDORA} {NO} {ENSINO} {PROFISSIONAL}},
	url = {http://www2.ifrn.edu.br/ojs/index.php/HOLOS/article/view/5264},
	doi = {10.15628/holos.2018.5264},
	abstract = {A educação empreendedora é uma ferramenta importante a ser disseminada entre as instituições de ensino, especialmente aquelas que atuam na educação profissional. Assim, o objetivo desse estudo foi diagnosticar e analisar a percepção dos alunos em relação às suas experiências nesta instituição, dentre estas, com a disciplina Gestão Organizacional. O empreendedorismo jovem no sentido amplo, em que exemplos práticos e ferramentas úteis são apresentados e utilizados pelos alunos, apresenta-se como forma de inclusão de jovens no mercado de trabalho e em contrapartida é fator preponderante para promoção do desenvolvimento socioeconômico brasileiro (LIMA-FILHO, SPROESSER e MARTINS, 2009; BULGACOV, 2010). Nesse sentido, esta pesquisa de natureza exploratória e descritiva utilizou um questionário com perguntas abertas e fechadas para a coleta de dados, seguido por uma análise quantitativa e qualitativa. Os questionários foram aplicados junto aos alunos dos cursos técnico integrado em alimentos e informática (nível médio – turmas concluintes do 4º ano) nos anos de 2012, 2013 e 2014 do IFRN – Campus Currais Novos, que cursaram a disciplina Gestão Organizacional, totalizando 215 alunos que compuseram o universo pesquisado. Os resultados indicaram que a experiência na instituição, bem como com a didática utilizada na disciplina, foram positivas.},
	language = {pt},
	number = {0},
	urldate = {2019-08-29},
	journal = {HOLOS},
	author = {Gomes, Danilo Cortez and Silva, Luciano Alexandre de Farias},
	month = feb,
	year = {2018},
	keywords = {Empreendedorismo, educação empreendedora, ensino profissional.},
	pages = {118--139}
}

