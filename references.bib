@article{collado_smart_2018,
	title = {Smart farming: {A} potential solution towards a modern and sustainable agriculture in {Panama}},
	copyright = {http://creativecommons.org/licenses/by/3.0/},
	shorttitle = {Smart farming},
	url = {http://www.aimspress.com/article/10.3934/agrfood.2019.2.266},
	doi = {10.3934/agrfood.2019.2.266},
	abstract = {Agriculture in the Tropical region, which includes Panama and other countries in the Caribbean and Latin America, has been developed using traditional tools with very little technology that has been designed and imported from countries with different environmental conditions. Some studies have suggested that the misuse of these tools may cause a negative impact on food production systems. Therefore, it is important to study, develop, and implement technological solutions that are appropriate for the conditions of specific regions and that help mitigating problems related to climate change and the production of food. One important solution is the implementation of technologies that interact with each other through the use of communication networks in Agriculture, known as “Smart Farming”. Many developed countries have promoted Research and Development (R\&D) in Smart Farming, providing great benefits to their food production sector. The objective of this work is to document and provide an overview of the current situation of the Agriculture in Panama and the opportunities and challenges in the study, development, and implementation of Smart Farming as a technological solution in the agricultural sector in this country, in order to achieve an intelligent agriculture that allows a modern and sustainable food production. Finally, this paper provides some recommendations for the successful implementation of Smart Farming in Panama.},
	language = {en},
	urldate = {2018-05-19},
	journal = {agriculture 2019, Vol. 4, Pages 266-284},
	author = {Collado, Edwin and Fossatti, Anibal and Saez, Yessica},
	month = mar,
	year = {2018}
}

@article{yoon_invention_2010,
	title = {Invention property-function network analysis of patents: a case of silicon-based thin film solar cells},
	volume = {86},
	issn = {0138-9130},
	shorttitle = {Invention property-function network analysis of patents},
	url = {https://akademiai.com/doi/abs/10.1007/s11192-010-0303-8},
	doi = {10.1007/s11192-010-0303-8},
	abstract = {Abstrato A análise de tecnologia é um processo que utiliza análise textual para detectar tendências na inovação tecnológica. A análise co-word (CWA), um método popular para análise de tecnologia, engloba (1) a definição de um conjunto de palavras-chave ou padrões de frase-chave representados em termos dependentes de tecnologia, (2) a geração de uma rede que codifica as relações entre ocorrências palavras-chave ou frases-chave e (3) identificar tendências específicas da rede. No entanto, a definição do conjunto de padrões de palavras-chave ou frases-chave depende muito do esforço de especialistas, que podem ser caros ou estarem indisponíveis. Além disso, a definição de padrões de palavras-chave ou frases-chave de áreas tecnológicas novas ou emergentes pode ser uma tarefa difícil, mesmo para especialistas. Para resolver a limitação no CWA, esta pesquisa adota uma abordagem baseada na função da propriedade. A propriedade é uma característica específica de um produto, e é geralmente descrito usando adjetivos; a função é uma ação útil de um produto e geralmente é descrita usando verbos. Propriedades e funções representam os conceitos de inovação de um sistema, por isso mostram direções inovadoras em uma determinada tecnologia. A metodologia proposta extrai automaticamente propriedades e funções de patentes usando o processamento de linguagem natural. Usando propriedades e funções como nós e co-ocorrências como links, uma rede de função de propriedade de invenção (IPFN) pode ser gerada. Usando a análise de redes sociais, a metodologia analisa as implicações tecnológicas dos indicadores no IPFN. Portanto, sem a predefinição de padrões de palavras-chave ou frases-chave, a metodologia ajuda os especialistas a se concentrarem mais em seus serviços de conhecimento que identificam as tendências em inovação tecnológica por meio de patentes.},
	number = {3},
	urldate = {2018-05-20},
	journal = {Scientometrics},
	author = {Yoon, Janghyeok and Choi, Sungchul and Kim, Kwangsoo},
	month = oct,
	year = {2010},
	pages = {687--703}
}

@incollection{magnaye_climate_2019,
	address = {Cham},
	series = {Advances in {Science}, {Technology} \& {Innovation}},
	title = {Climate {Smart} {Agriculture} {Edu}-tourism: {A} {Strategy} to {Sustain} {Grassroots} {Pro}-biodiversity {Entrepreneurship} in the {Philippines}},
	isbn = {978-3-030-10804-5},
	shorttitle = {Climate {Smart} {Agriculture} {Edu}-tourism},
	url = {https://doi.org/10.1007/978-3-030-10804-5_20},
	abstract = {The wide array of economic activities that sustain income generation creates human pressures that lead to biodiversity and habitat loss, rapid depletion of natural resources, environmental pollution, and climate change, among others. To dissuade the alarming rate of environmental degradation, pro-biodiversity enterprises are promoted and supported in the Philippines. These enterprises conserve biodiversity through the adoption of sustainable use practices and the utilization of biological resources toward equitable sharing of benefits. Organic farming is considered a pro-biodiversity enterprise. It is categorized by the Philippine Department of Agriculture under the Climate Smart Agriculture (CSA). CSA encompasses the application of climate-smart principles in agriculture to enhance the productivity and income, develop resiliency to climate change, and reduce or eliminate greenhouse gas emissions. There are a number of grassroots or community-based pro-biodiversity agri-entrepreneurs in the Philippines that complement their farm operations with education tourism activities or farm “edu-tourism” (rural tourism or agri-tourism). It is where tourists including entrepreneurs and technology adopters visit an agricultural farm to engage in learning experiences, meet with farmers, and understand their culture as well as practices in food production and marketing. CSA edu-tourism provides income to farmers from the sales of farm produce and demonstration of farm activities. The study defines the value-added features of CSA edu-tourism to planning, implementing, and managing community-based pro-biodiversity enterprises in the Philippines. The in-depth interview with organic farmers and entrepreneurs provided the multifarious benefits of CSA edu-tourism and emphasized how it promotes harmonization of communities with nature. The study also demonstrated the relationship between edu-tourism and grassroots pro-biodiversity entrepreneurship considering the environmental conservation approach of CSA. Further, the study determined the potential financial and cultural gains of pro-biodiversity entrepreneurs from CSA edu-tourism as basis in planning sustainable, climate-sensitive, and biodiversity-friendly grassroots enterprises.},
	language = {en},
	urldate = {2018-05-03},
	booktitle = {Cultural {Sustainable} {Tourism}: {A} {Selection} of {Research} {Papers} from {IEREK} {Conference} on {Cultural} {Sustainable} {Tourism} ({CST}), {Greece} 2017},
	publisher = {Springer International Publishing},
	author = {Magnaye, Dina Cartagena},
	editor = {Stankov, Uglješa and Boemi, Sofia-Natalia and Attia, Sahar and Kostopoulou, Stella and Mohareb, Nabil},
	year = {2019},
	doi = {10.1007/978-3-030-10804-5_20},
	keywords = {Agri-tourism, Biodiversity, Biodiversity-friendly business, Edu-tourism, Education tourism, Farm tourism, Grassroots pro-biodiversity, Pro-biodiversity enterprises, Pro-biodiversity entrepreneurship},
	pages = {203--218}
}

@article{bemfica_disseminacao_2019,
	title = {{DISSEMINAÇÃO} {DA} {PROPRIEDADE} {INTELECTUAL} {COMO} {ESTRATÉGIA} {PARA} {POLÍTICAS} {DE} {CIÊNCIA}, {TECNOLOGIA} {E} {INOVAÇÃO}: {O} {CASO} {DO} {SISTEMA} {PERNAMBUCANO} {DE} {INOVAÇÃO} ({SPIn})},
	volume = {12},
	copyright = {Direitos autorais 2019 Cadernos de Prospecção},
	issn = {2317-0026},
	shorttitle = {{DISSEMINAÇÃO} {DA} {PROPRIEDADE} {INTELECTUAL} {COMO} {ESTRATÉGIA} {PARA} {POLÍTICAS} {DE} {CIÊNCIA}, {TECNOLOGIA} {E} {INOVAÇÃO}},
	url = {https://portalseer.ufba.br/index.php/nit/article/view/27222},
	doi = {10.9771/cp.v12i1.27222},
	abstract = {A disseminação do conhecimento em Propriedade Intelectual (PI) entre todos os atores que compõem o Sistema Regional de Inovação (SRI) é essencial para a promoção de uma cultura dentro dos negócios inovadores. Este trabalho busca propor ações de disseminação do conhecimento em PI para as instituições que compõem o Sistema Pernambucano de Inovação (SPIn) mapeadas no documento Estratégia de Ciência, Tecnologia e Inovação para Pernambuco 2017 – 2022 (ECT\&I-PE). A metodologia utilizada foi uma abordagem qualitativa, de caráter descritivo, considerando as dimensões dos sistemas regionais de inovação discutidas por Souza Júnior (2014): científica, tecnológica, intermediação, capacitação e gestão empresarial, financiamento e governança, agrupando-se às instituições do SPIn dentro de cada uma das dimensões. Como resultado, foram realinhados aos seis eixos estratégicos do documento que nortearam as propostas para o fomento de políticas públicas destinadas à inovação por meio da disseminação do conhecimento em PI junto ao SPIn.},
	language = {pt-BR},
	number = {1},
	urldate = {2018-05-03},
	journal = {Cadernos de Prospecção},
	author = {Bemfica, Eduardo Andrade and Cavalcanti, André Marques},
	month = mar,
	year = {2019},
	pages = {15}
}

@article{santos_fibra_2019,
	title = {A {FIBRA} {DE} {COCO} {COMO} {MATÉRIA}-{PRIMA} {PARA} {O} {DESENVOLVIMENTO} {DE} {PRODUTOS}: {UMA} {PROSPECÇÃO} {TECNOLÓGICA} {EM} {BANCOS} {DE} {PATENTES}},
	volume = {12},
	copyright = {Direitos autorais 2019 Cadernos de Prospecção},
	issn = {2317-0026},
	shorttitle = {A {FIBRA} {DE} {COCO} {COMO} {MATÉRIA}-{PRIMA} {PARA} {O} {DESENVOLVIMENTO} {DE} {PRODUTOS}},
	url = {https://portalseer.ufba.br/index.php/nit/article/view/27230},
	doi = {10.9771/cp.v12i1.27230},
	abstract = {O uso do coco vai muito além do que é encontrado e extraído do seu interior, tendo a sua parte externa elevado potencial de aproveitamento industrial. A fibra retirada da casca do coco vem sendo utilizada para muitas finalidades capazes de melhorar o desenvolvimento econômico bem como o desenvolvimento sustentável. O presente artigo realiza uma análise de tecnologias depositadas em bases de patentes através do software ORBIT® relacionadas à fibra de coco. Os resultados obtidos demonstram inclinações para estratégias empresariais, bem como de governos e instituições de pesquisas nacionais e internacionais, com um gradativo interesse pelo investimento em tecnologias vinculadas ao tema. Verifica-se que a fibra de coco tem capacidade para ser explorada em pesquisas futuras diante da sua versatilidade, suprindo demandas da sociedade aliada à preservação ambiental},
	language = {pt-BR},
	number = {1},
	urldate = {2018-05-03},
	journal = {Cadernos de Prospecção},
	author = {Santos, Djane Encarnação dos and Martinez, Felipe Carlos Carvalho and Juiz, Paulo José Lima},
	month = mar,
	year = {2019},
	pages = {153}
}

@article{santos_prospeccao_2019,
	title = {{PROSPECÇÃO} {TECNOLÓGICA} {SOBRE} {BIOMARCADORES} {RELACIONADOS} {COM} {DIAGNÓSTICO} {DE} {DOENÇAS} {PSIQUIÁTRICAS}},
	volume = {12},
	copyright = {Direitos autorais 2019 Cadernos de Prospecção},
	issn = {2317-0026},
	url = {https://rigs.ufba.br/index.php/nit/article/view/27224},
	doi = {10.9771/cp.v12i1.27224},
	abstract = {Os transtornos mentais configuram-se como uma doença que possui características psicológicas associadas a comprometimento funcional resultante de disfunção biopsicossocial. Métodos de detecção com a utilização de biomarcadores que permitam e que gerem resultados rápidos, seguros e reprodutíveis são necessários para auxiliar os profissionais de saúde na avaliação e monitoramento das doenças psiquiátricas. Foram incluídas as descrições das patentes que abordassem o desenvolvimento de um biomarcador aplicado para o diagnóstico de doenças psiquiátricas, posteriormente foram excluídas as patentes repetidas na mesma base, patentes de Adição e que abordassem métodos de imagem e com enfoque em doenças neurológicas. A prospecção mostrou que a maioria dos depósitos de patentes envolvendo tecnologias para comorbidades psiquiátricas, teve um aumento crescente nos últimos anos principalmente a partir de 2012 tendo os Estados Unidos como maior país depositante e acúmulo de inventores, com pouco desenvolvimento tecnológico sobre a temática no Brasil.},
	language = {pt-BR},
	number = {1},
	urldate = {2018-05-03},
	journal = {Cadernos de Prospecção},
	author = {Santos, Ana Caroline Melo dos and Silva, Denise Macedo da and Santos, Bruna Brandão dos and Farias, Karol Fireman de and Figueiredo, Elaine Virginia Martins de Souza},
	month = mar,
