@article{cortes-landazury_degradation_2017,
	title = {From degradation of water and institutional innovations to the greening civil society: {An} analysis of the problems of water quality in the high {Cauca} {River} {Basin}, {Colombia}},
	volume = {45},
	issn = {01225391 (ISSN)},
	shorttitle = {De la degradacion hidrica y las innovaciones institucionales, a la sociedad civil ambientalizada: {Un} analisis de la {problemÁtica} de la calidad del agua en la cuenca alta del rió cauca, {Colombia}},
