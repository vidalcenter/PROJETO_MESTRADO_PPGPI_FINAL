@article{tavares_alise_2019,
	title = {Análise multivariada de espécies florestais nativas em relação aos atributos químicos e texturais do solo na região de {Cotriguaçu} – {MT}},
	volume = {29},
	copyright = {Direitos autorais 2019 Ciência Florestal},
	issn = {1980-5098},
	url = {https://periodicos.ufsm.br/cienciaflorestal/article/view/3577},
	doi = {10.5902/198050983577},
	abstract = {Objetivou-se neste trabalho classificar as espécies florestais quanto às exigências em fertilidade do solo, bem como identificar grupos com características semelhantes utilizando técnicas da estatística multivariada. Para isso, foram analisadas amostras de solo quanto à textura e aos atributos químicos e realizado um levantamento fitossociológico da região. Na análise de componentes principais, as relações do complexo solo foram resumidas em quatro componentes que juntos explicaram 80\% da variância dos dados, com relações mais significativas entre a soma de bases, saturação por bases, capacidade de troca catiônica efetiva, saturação por alumínio, Ca+Mg, Al e argila, com coeficiente de correlação maior que 90\%. Na análise de agrupamento hierárquico, com 70\% de similaridade quanto aos atributos do solo, formaram-se três grupos distintos de espécies florestais. A análise multivariada foi eficiente na redução das variáveis originais, separação e classificação dos grupos e identificação dos atributos químicos do solo mais requeridos pelas espécies florestais. Os Grupos 1 e 2 contêm espécies mais exigentes em fertilidade, com preferências por solos de textura média ricos em bases de cálcio e magnésio, enquanto o Grupo 3 parece ser menos exigente, prevalecendo em solos mais arenosos e mais ácidos, com grande capacidade de troca de cátions e elevada saturação por alumínio; podendo, as espécies desse último grupo, ser indicadas para recuperação de áreas degradadas. As espécies Copaifera sp. e Goupia glabra merecem mais estudos quanto aos mecanismos de adaptação às condições de baixa fertilidade do solo.},
	language = {pt},
	number = {1},
	urldate = {2018-05-04},
	journal = {Ciência Florestal},
	author = {Tavares, Lorena de Souza and Weber, Oscarlina Lúcia dos Santos and Valadão, Franciele Caroline de Assis and Espinosa, Mariano Martínez},
	month = apr,
	year = {2019},
	keywords = {Análise de agrupamentos, Análise de componentes principais, Análise discriminante, Cluster analysis, Discriminant analysis, Principal component analysis, Reflorestamento, Reforestation},
	pages = {281--291}
}

