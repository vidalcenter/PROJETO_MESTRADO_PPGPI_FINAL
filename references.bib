	journal = {Cadernos de Prospecção},
	author = {Vinhas Porto, Paula Marques Borges and Tosta, Silvana Sandes and Quintella, Cristina M. and De Jesus, Thaison Monteiro and Rocha, Washigton Franca},
	month = dec,
	year = {2018},
	pages = {1627}
}

