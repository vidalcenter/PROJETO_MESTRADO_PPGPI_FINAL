@article{wallis_institutional_2011,
	title = {Institutional change in multi-scalar water governance regimes: {A} case from {Victoria}, {Australia}},
	volume = {22},
	issn = {14785277 (ISSN)},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84865144252&partnerID=40&md5=8353ca095e4b872ed81e6c695ac7549c},
	abstract = {This article describes some of the key institutional and historical changes in water and natural resources management from a multi-scalar federal, state and regional perspective, and critically analyzes them through the theoretical lens of transition theory. The nested water and natural resource governance regime across Australia's Murray-Darling Basin, the State of Victoria and the Goulbum-Broken catchment region can be understood to have experienced two major transitions. The first, from a state-based 'nation-building' regime to a more regionally-focused cooperative governance regime, occurred in response to the key landscape drivers of increasing environmental concern and user-pays economic rationalism, prompted by niche-innovation in regional governance. The second, to a federally-focused 'centralized governance' regime, was in response to the landscape drivers of water shortage, overallocation of water resources and resultant environmental degradation, and was institutionalized by legislation and water policy at federal and state levels. The use of legislation has implications for transition pathways and the centralization of decision-making power within the nested set of local, regional, state and federal governance arrangements, as only state and federal levels have the power to legislate.},
	language = {English},
	number = {2-3},
