@article{guo_what_2019,
	title = {What determines academic entrepreneurship success? {A} social identity perspective},
	volume = {15},
	issn = {1555-1938},
	shorttitle = {What determines academic entrepreneurship success?},
	url = {https://doi.org/10.1007/s11365-019-00569-6},
	doi = {10.1007/s11365-019-00569-6},
	abstract = {As universidades e institutos são cada vez mais reconhecidos como fontes importantes nos sistemas nacionais de inovação. Como tal, um número crescente de acadêmicos está participando de atividades empresariais e outras atividades comerciais, e o tópico do empreendedorismo acadêmico tem atraído ampla atenção. Este artigo tem como objetivo explorar o efeito da identificação empreendedora no empreendedorismo acadêmico do ponto de vista da teoria da identidade social e considerar os efeitos do contexto (inércia do capital social, narrativa empreendedora). Com base em 248 amostras de empreendedor acadêmico, os resultados empíricos indicam que a relação entre a identificação empresarial e o desempenho do empreendedorismo acadêmico é positiva. Além do que, além do mais, o relacionamento acima mencionado é negativamente moderado pela inércia do capital social, enquanto positivamente moderado pela narrativa empreendedora. Além disso, a identificação empreendedora é melhor para o desempenho do empreendedorismo acadêmico no contexto de baixos níveis de inércia do capital social e altos níveis de narrativa empreendedora. Contribuições teóricas, implicações práticas, limitações e pesquisas futuras são discutidas.},
	language = {en},
	number = {3},
	urldate = {2019-09-14},
	journal = {International Entrepreneurship and Management Journal},
	author = {Guo, Feng and Zou, Bo and Guo, Jinyu and Shi, Yan and Bo, Qingwen and Shi, Liangxing},
	month = sep,
	year = {2019},
	keywords = {Desempenho, Empreendedorismo acadêmico, Identidade, Inércia do capital social, Narrativa empreendedora},
	pages = {929--952}
}

