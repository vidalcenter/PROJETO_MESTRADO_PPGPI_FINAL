	journal = {Cadernos de Prospecção},
	author = {Fernandes, Thalys de Freitas and Souza, Caio Cefas da Nóbrega and Alexandre, Gerônimo Barbosa and Silva, José Nilton},
	month = jun,
	year = {2019},
	pages = {388}
}

