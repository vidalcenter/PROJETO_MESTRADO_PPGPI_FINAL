	keywords = {Adaptation, Adoption, Agriculture, Climate, Cultivation, Economics, Freshwater supply, Groundwater, Groundwater resources, Investments, Knowledge, Netherlands, Planning, Surveys, Water, adaptive management, agriculture, climate change, freshwater, knowledge, socioeconomic impact, water resource, water supply},
