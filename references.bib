	author = {Jager, N.W. and Challies, E. and Kochskämper, E. and Newig, J. and Benson, D. and Blackstock, K. and Collins, K. and Ernst, A. and Evers, M. and Feichtinger, J. and Fritsch, O. and Gooch, G. and Grund, W. and Hedelin, B. and Hernández-Mora, N. and Hüesker, F. and Huitema, D. and Irvine, K. and Klinke, A. and Lange, L. and Loupsans, D. and Lubell, M. and Maganda, C. and Matczak, P. and Parés, M. and Saarikoski, H. and Slavíková, L. and Van Der Arend, S. and Von Korff, Y.},
	year = {2016},
