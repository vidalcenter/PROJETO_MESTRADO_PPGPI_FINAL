@article{mutyasira_prospects_2020,
	title = {Prospects of sustainable intensification of smallholder farming systems: {A} farmer typology approach},
	issn = {2042-1338},
	shorttitle = {Prospects of sustainable intensification of smallholder farming systems},
	url = {https://www-tandfonline.ez20.periodicos.capes.gov.br/doi/full/10.1080/20421338.2019.1711319},
	doi = {10.1080/20421338.2019.1711319},
	abstract = {Smallholder farming systems are highly diverse, owing to differences in farmers’ resource endowments, levels of technology use, degree of market access and agro-ecological factors. Consequently, the prospects of achieving sustainable intensification of smallholder farming systems need to be examined vis-à-vis the dominant farmer typologies, so as to help craft effective and tailored interventions. The objective of this paper is to develop a methodology to establish farm typologies prevalent within the smallholder farming systems and infer their implications for the drive to achieve sustainable intensification of agriculture. A cross-sectional survey was carried out, covering 600 households in four regions of Ethiopia’s Highlands. Multivariate statistical techniques of Principal Components Analysis (PCA) and Cluster Analysis (CA) were used to group farmers into four distinct clusters. The study then compared relative levels of agricultural intensification and sustainability across the different typology clusters. The study found that households who are more commercially oriented and have larger landholdings, higher levels of productive assets and livestock ownership, exhibit higher levels of both agricultural intensification and relative sustainability. The study concludes that the farm typology approach has important policy application as it helps identify the set of socioeconomic characteristics of farmers that influence their propensity to achieve sustainable intensification.},
	urldate = {2020-08-16},
	journal = {African Journal of Science, Technology, Innovation and Development},
	author = {Mutyasira, Vine},
	month = feb,
	year = {2020},
	note = {Publisher: Routledge},
	pages = {1--8}
}

@article{abisuga-oyekunle_smes_2020,
	title = {{SMEs} in sustainable development: {Their} role in poverty reduction and employment generation in sub-{Saharan} {Africa}},
	volume = {12},
	issn = {2042-1338},
	shorttitle = {{SMEs} in sustainable development},
	url = {https://www-tandfonline.ez20.periodicos.capes.gov.br/doi/full/10.1080/20421338.2019.1656428},
	doi = {10.1080/20421338.2019.1656428},
	abstract = {A redução da pobreza ocupou um lugar de destaque na agenda de desenvolvimento internacional após a adoção dos Objetivos de Desenvolvimento do Milênio das Nações Unidas (ODM) em 2000. Os países africanos iniciaram várias missões para estabelecer um crescimento econômico sustentado aumentando a produtividade com o objetivo final de redução da pobreza. A importância das pequenas e médias empresas (PMEs) na redução da pobreza e na geração de empregos é amplamente reconhecida. Apesar da elevada taxa de insucesso entre as PME, particularmente no contexto africano, algumas estão a tentar obter excelentes resultados e a demonstrar sinais de crescimento, inovação e sustentabilidade. Este artigo explora o desenvolvimento das PMEs como uma medida estratégica para a criação de empregos na África Subsaariana. Ele examina o papel das PMEs na obtenção de empregos sustentáveis ​​na África. Este estudo irá beneficiar os governos da África, levando-os a se concentrarem no desenvolvimento das PMEs e orientando-os sobre a melhor forma de integrar o desenvolvimento das PMEs para alcançar o alívio da pobreza. As recomendações propostas neste estudo podem ser úteis na formulação de políticas, operação de negócios e práticas regulatórias para o crescimento e desenvolvimento sustentáveis ​​no contexto da África Subsaariana.},
	number = {4},
	urldate = {2020-08-16},
	journal = {African Journal of Science, Technology, Innovation and Development},
	author = {Abisuga-Oyekunle, Oluwayemisi Adebola and Patra, Swapan Kumar and Muchie, Mammo},
	month = jun,
	year = {2020},
	note = {Publisher: Routledge},
	pages = {405--419}
}

