	author = {Mendonça, T. A. and Dantas, E. A. and Gouvea, A. A.L. and Oliveira, R. S. and Alves, A. R.C. and Padilha, F. F. and Machado, B. A.S.},
	year = {2016},
	keywords = {Flavor, Patents, Technological trends},
