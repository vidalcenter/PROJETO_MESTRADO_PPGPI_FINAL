@article{de_castro_evolution_2015,
	title = {The evolution of international water law},
	volume = {10},
	issn = {17437601 (ISSN)},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84954553388&doi=10.2495%2fSDP-V10-N6-894-900&partnerID=40&md5=5fb540ee26eee3063ddc2f24cbe20c8c},
	doi = {10.2495/SDP-V10-N6-894-900},
	abstract = {International Water Law has been witnessing deep changes in the latter two decades. These changes denote certain cardinal options for new values that the newer International Water Law is seeking to embrace. Amongst these, the ones of environmentalization, humanization and economicization stand out as attempts at bringing efficient response to the challenges set by the current global water crisis. They amount to true paradigm shifts in the understanding of International Water Law and the normative message conveyed. © 2015 WIT Press.},
	language = {English},
	number = {6},
	journal = {International Journal of Sustainable Development and Planning},
	author = {De Castro, P.C.},
