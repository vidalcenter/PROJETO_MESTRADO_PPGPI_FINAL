@book{aveni_empreendedorismo_2019,
	title = {{EMPREENDEDORISMO} {PARA} {CRIANÇAS} {E} {ADOLESCENTES} {NO} {BRASIL}. {COMPARAÇÃO} {DE} {METODOS}},
	volume = {12},
