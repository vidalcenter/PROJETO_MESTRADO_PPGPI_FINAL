	author = {Quintella, Vitor M. and Quintella, Cristina M. and Junior, Antônio Francisco A. Silva and Fontes, Cristiano de Oliveira Hora},
	month = mar,
	year = {2019},
	keywords = {Aprendizagem de máquina, Investimento, Mercado financeiro, Negociação.},
