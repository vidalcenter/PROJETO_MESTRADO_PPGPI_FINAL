	number = {3-4},
	journal = {Ecohydrology and Hydrobiology},
	author = {Fritz, P.},
	year = {2011},
	keywords = {ecohydrology, economic activity, economic dimention, ecosystem services, environmental policy, evnironmental policy, land management, natural resource, protection, water management, water resource},
	pages = {141--144}
}

@article{blancher_ecosystem_2011,
	title = {Ecosystem services approach for water framework directive implementation},
	volume = {148},
	issn = {17433541 (ISSN); 9781845645328 (ISBN)},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84865716381&doi=10.2495%2fRAV110081&partnerID=40&md5=996e56f17aac490e72c362d6ff2f8127},
	doi = {10.2495/RAV110081},
	abstract = {The modern concept of 'ecosystem services' has progressed significantly in recent decades. Conceived primarily as a communication tool in the late 1970s to explain societal dependence on nature, it now incorporates economic dimensions and provides help to decision makers for implementing effective conservation policies which support human wellbeing and sustainable development. The founding work appeared in the late 1980s led to the conducting by policymakers of a more systematic assessment of the net monetary value associated with the preservation or restoration of natural areas. Following this work, many case studies have highlighted that ignorance of the value of natural capital into decisions on land use and resources allocation most likely results in degradation and destruction of this natural capital and eventually prove very costly for society. In this paper is presented a research project that investigates the methodological links of the two concepts (Ecosystems Services Approach (ESA) and Water Framework Directive (WFD) - economics). Its main academic innovation will be on assessing the potential "added value" of using the ESA approach and results in Integrated Water Resource Management policies decision and implementation processes, and the related communication and stakeholders' participation, with a specific focus to WFD. © 2011 WIT Press.},
