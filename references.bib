	title = {{ATRIBUTOS} {FÍSICOS} {DO} {SOLO} {EM} {SISTEMA} {CONSORCIADO} {E} {MONOCULTIVO} {DO} {MARACUJÁ} ({Passiflora} edulis sims)},
