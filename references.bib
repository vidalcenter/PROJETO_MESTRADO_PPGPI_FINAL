@article{bispo_soil_2017,
	title = {Soil, water, nutrients and soil organic matter losses by water erosion as a function of soil management in the {Posses} sub-watershed, {Extrema}, {Minas} {Gerais}, {Brazil}},
	volume = {38},
	copyright = {Copyright (c) 2017 Semina: Ciências Agrárias},
	issn = {1679-0359},
	url = {http://www.uel.br/revistas/uel/index.php/semagrarias/article/view/26562},
	doi = {10.5433/1679-0359.2017v38n4p1813},
	abstract = {Knowledge of the quantity and quality of the material lost by soil erosion due to soil management is a basic need to identify land management zones in catchments. The aim of this study was to investigate the influence of soil management on the quantity and quality of soil material lost by erosion in the Posses sub-watershed, Municipality of Extrema, State of Minas Gerais, Brazil. Water and sediments lost by natural rainfall erosion were sampled from erosion plots located on a Red-Yellow Argisol (PVA) under the following systems: bare soil, subsistence farming (maize/beans/pumpkin/jack-beans/fallow), degraded pasture, well-managed pasture, and reforestation set up in 2013; and in a Litholic Neosol (RL): reforestation set up in 2008, bare soil, and native forest. Ca, Mg, K, P, N and soil organic matter (SOM) contents were determined in sediment and soil samples (at 0-5 cm depth) for the determination of the runoff enrichment ratios. Management influences soil losses more so than water losses. Minor losses were found in reforestation set up in 2013 (soil); in well-managed pasture (water); and in reforestations (nutrients and SOM). These losses tend to stability with time. The general sequence of nutrient losses was N {\textgreater} Ca {\textgreater} Mg {\textgreater} K {\textgreater} P in PVA; and N {\textgreater} Ca {\textgreater} K {\textgreater} Mg {\textgreater} P in RL. Loss rates of SOM and N followed the order: bare soil {\textgreater} subsistence farming {\textgreater} degraded pasture {\textgreater} well-managed pasture {\textgreater} reforestation, in PVA; and bare soil {\textgreater} native forest {\textgreater} reforestation, in RL. Reforestation and well-management pasture are effective conservation strategies in order to lower the erosion process in the Posses sub-watershed. Soil losses, as well as nutrients and organic matter losses were more influenced by soil management than water losses. The safeguarding native forest under Litholic Neosol is essential to the conservation of this pedoenvironment, especially in steep slopes.},
	language = {en},
	number = {4},
	urldate = {2018-05-12},
	journal = {Semina: Ciências Agrárias},
	author = {Bispo, Diêgo Faustolo Alves and Silva, Marx Leandro Naves and Pontes, Lucas Machado and Guimarães, Danielle Vieira and Marques, João José Granate de Sá e Melo and Curi, Nilton},
	month = aug,
	year = {2017},
	keywords = {Conservation management, Degradação do solo, Environmental quality, Manejo conservacionista, Qualidade ambiental, Segurança do solo e da água., Soil and water safety., Soil degradation},
	pages = {1813--1824}
}

