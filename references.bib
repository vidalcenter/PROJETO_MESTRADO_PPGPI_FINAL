	keywords = {Constitutionality, Integrated water resources management, Jalisco, Mexico, Mexico [North America], Sustainable water governance, Water justice, accessibility, governance approach, innovation, institutional framework, perception, sustainability, water management, water resource},
	pages = {51--64}
}

