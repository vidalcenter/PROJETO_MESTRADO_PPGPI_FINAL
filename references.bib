@article{cicek_effects_2019,
	title = {The effects of different types of fibres and geotextiles for pavement design},
	volume = {20},
	issn = {1468-0629},
	url = {https://www-tandfonline.ez20.periodicos.capes.gov.br/doi/full/10.1080/14680629.2017.1417890},
	doi = {10.1080/14680629.2017.1417890},
	abstract = {Este artigo define os testes de compressão triaxial realizados para descrever os comportamentos de diferentes tipos de reforço e as novas técnicas de estabilização de fibra para uma sub-base de rodovia. Para investigar as interações entre o reforço e as partículas de areia, três tipos de fibras e dois tipos diferentes de geotêxteis foram usados ​​como reforços em um conjunto abrangente de testes. Primeiramente, o solo arenoso foi misturado com diferentes tipos de fibras e os resultados foram comparados com uma condição não reforçada. Então, como uma nova técnica de estabilização, as configurações das fibras no solo foram alteradas. Camadas de uma ( N  = 1) e multi-armadura ( N = 2 e 3) foram utilizados para cada tipo de fibra sem aumentar a quantidade de fibras. Além disso, os resultados para as amostras de reforço de fibra foram discutidos com diferentes modelos de areia reforçada com geotêxtil. Um objetivo secundário do artigo é determinar a influência das variáveis ​​de reforço e da técnica de estabilização nos padrões de falha e outros comportamentos, como o índice de fragilidade e o módulo de elasticidade. Como resultado, observou-se que a técnica de estabilização e o tipo de reforço têm um papel importante nos comportamentos compostos. As fórmulas determinadas neste estudo apresentam uma boa correlação entre os comportamentos de estresse médio e desvios nos resultados do experimento.},
	number = {4},
	urldate = {2020-01-26},
	journal = {Road Materials and Pavement Design},
	author = {Cicek, Elif},
	month = may,
	year = {2019},
	note = {publisher-place: Ankara-TK},
	pages = {793--814}
}

