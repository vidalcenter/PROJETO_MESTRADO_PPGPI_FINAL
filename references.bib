	year = {2017},
	note = {publisher-place: German-DE},
	keywords = {Bitumen coating, Jute geotextiles, Serviceability, Transesterification, Unpaved roads},
	pages = {47--54}
}

@article{richaud_accelerated_2008,
	title = {Accelerated ageing of polypropylene geotextiles, the effect of temperature, oxygen pressure and aqueous media on fibers—{Methodological} aspects},
