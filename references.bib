@article{zhu_field_2020,
	title = {Field {Observations} of {Wind} {Waves} in {Upper} {Delaware} {Bay} with {Living} {Shorelines}},
	issn = {1559-2731},
	url = {https://doi.org/10.1007/s12237-019-00670-7},
	doi = {10.1007/s12237-019-00670-7},
	abstract = {AbstratoOs recifes de ostras construídos (CORs) fornecem proteções e habitats em terra para comunidades de peixes e moluscos através da atenuação da energia das ondas. No entanto, os processos e o mecanismo dos CRs na atenuação das ondas permanecem obscuros, limitando assim a avaliação eficaz dos CRs para a proteção da linha costeira. Este artigo apresenta os resultados de uma investigação de campo sobre as características das ondas e as variações espectrais das ondas ao longo de uma costa com CORs em um estuário com uma grande faixa de marés, assim como grandes ondas de vento e energia das ondas. Seis transdutores de pressão foram implantados de 31 de janeiro a 2 de abril de 2018, em Gandy's Beach, Nova Jersey, na parte superior da baía de Delaware. Os CRs foram construídos no local do estudo em 2016 como estruturas vivas da costa após o furacão Sandy. Os dados coletados no local do estudo exibem as variações de ondas e as características espectrais ao longo de 2 meses, incluindo quatro tempestades de inverno (ou seja, nor'easters). As variações espaciais das alturas das ondas medidas em ambos os lados dos CORs mostram uma forte dependência da razão entre o bordo livre dos CORs e as alturas das ondas offshore. Devido à grande amplitude de maré ({\textgreater} 2 m), as cristas dos CRs permanecem submersas em 85\% das vezes. Os CORs submersos fornecem apenas atenuação parcial da energia das ondas. O ambiente das ondas no estuário é complexo, especialmente durante nor'easters. Por exemplo, ventos com busca rápida de mudanças podem levar a mares eólicos bimodais. Devido aos complexos espectros de ondas, as alturas das ondas em massa, como as alturas significativas das ondas, não podem ser adotadas para revelar adequadamente a capacidade dos CRs de atenuar a energia das ondas. A análise espectral é conduzida para investigar as variações espaciais e temporais da energia das ondas em caixas de frequências direcionadas. Os resultados da análise espectral revelam a transferência de energia das ondas primárias para os altos harmônicos depois que as ondas se propagam sobre os CORs submersos. Além disso, verificou-se que a energia das ondas originárias do Oceano Atlântico pode penetrar nos CRs sem amortecimento, mesmo quando emergentes. Este estudo pode ajudar os gerentes de recursos a avaliar em profundidade a eficácia da linha costeira viva e melhorar as estruturas da linha costeira viva, como os CRs.},
	language = {en},
	urldate = {2020-02-12},
	journal = {Estuaries and Coasts},
	author = {Zhu, Ling and Chen, Qin and Wang, Hongqing and Capurso, William and Niemoczynski, Lukasz and Hu, Kelin and Snedden, Gregg},
	month = jan,
	year = {2020},
	note = {publisher-place: Texas-EUA},
	keywords = {Análise espectral, Costa viva, Energia das, Nor'easters, Ondas de, Recifes de ostras construídos, Upper Delaware Bay, ondas, vento}
}

@article{liang_capillary-gravity_2017,
