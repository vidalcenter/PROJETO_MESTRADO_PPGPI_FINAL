	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-84872848015&doi=10.1016%2fj.advwatres.2012.01.005&partnerID=40&md5=8d01e2aae46e860a9cdb21225a14acf5},
	doi = {10.1016/j.advwatres.2012.01.005},
	abstract = {This study contributes a rigorous diagnostic assessment of state-of-the-art multiobjective evolutionary algorithms (MOEAs) and highlights key advances that the water resources field can exploit to better discover the critical tradeoffs constraining our systems. This study provides the most comprehensive diagnostic assessment of MOEAs for water resources to date, exploiting more than 100,000 MOEA runs and trillions of design evaluations. The diagnostic assessment measures the effectiveness, efficiency, reliability, and controllability of ten benchmark MOEAs for a representative suite of water resources applications addressing rainfall-runoff calibration, long-term groundwater monitoring (LTM), and risk-based water supply portfolio planning. The suite of problems encompasses a range of challenging problem properties including (1) many-objective formulations with four or more objectives, (2) multi-modality (or false optima), (3) nonlinearity, (4) discreteness, (5) severe constraints, (6) stochastic objectives, and (7) non-separability (also called epistasis). The applications are representative of the dominant problem classes that have shaped the history of MOEAs in water resources and that will be dominant foci in the future. Recommendations are given for the new algorithms that should serve as the benchmarks for innovations in the water resources literature. The future of MOEAs in water resources needs to emphasize self-adaptive search, new technologies for visualizing tradeoffs, and the next generation of computing technologies. © 2012 Elsevier Ltd.},
	language = {English},
	journal = {Advances in Water Resources},
	author = {Reed, P.M. and Hadka, D. and Herman, J.D. and Kasprzyk, J.R. and Kollat, J.B.},
	year = {2013},
	keywords = {Assessment measures, Benchmarking, Commerce, Computing technology, Decision support, Decision support systems, Decision supports, Design evaluation, Evolutionary algorithms, Evolutionary multiobjective optimization, Groundwater, Groundwater monitoring, Groundwater resources, Hydrologic calibration, Multi-modality, Multiobjective evolutionary algorithms, Multiobjective optimization, Non-separability, Portfolio planning, Rainfall runoff, Risk management, Risk-based, Self-adaptive, Water supply, algorithm, assessment method, calibration, decision support system, groundwater, monitoring, risk assessment, trade-off, water resource},
	pages = {438--456}
}

