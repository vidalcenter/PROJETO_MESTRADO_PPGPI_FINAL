@article{saude_alternative_2012,
	title = {Alternative measures for biospeckle image analysis},
	volume = {29},
	copyright = {\&\#169; 2012 Optical Society of America},
	issn = {1520-8532},
	url = {https://osapublishing.ez20.periodicos.capes.gov.br/josaa/abstract.cfm?uri=josaa-29-8-1648},
	doi = {10.1364/JOSAA.29.001648},
	abstract = {Biospeckle is a technique whose purpose is to observe and study the underlying activity of some material. It has its roots in optical physics, and its first step is an image acquisition process that produces a video sequence of the reflection of a laser. The video content can be analyzed to have an interpretation of the activity of the observed material. The literature on this subject presents several different measures for analyzing the video sequence. Three of the most popular measures are the generalized difference (GD), the weighted generalized difference (WGD), and Fujii\&\#x2019;s method. These measures have drawbacks such as high computation time or limited visual quality of the results. In this paper, we propose (i)\&\#xA0;an alternative O(n) algorithm for the computation of the GD, (ii)\&\#xA0;an alternative measure based on the GD, (iii)\&\#xA0;an alternative measure based on the WGD, and (iv)\&\#xA0;a generalized definition of the Fujii\&\#x2019;s method with better visual quality. We discuss the similarities between the new measures and the existent ones, showing when they are applicable. We prove the gain in time computation. The proposed measures will help researchers to gain time during their research and to be able to develop faster tools based on biospeckle application.},
	language = {EN},
	number = {8},
	urldate = {2020-05-25},
	journal = {JOSA A},
