	author = {Mulligan, M.},
	year = {2013},
	keywords = {Adaptation to climate changes, Climate change, GIS, Geographic information systems, Heterogeneous environments, Hydrological model, Hydrological modeling, Hydrology, Land-cover change, Physically based modeling, Policy support, Remote sensing, Spatially explicit, Very high spatial resolutions, Water resources, data set, environmental policy, hydrological modeling, land cover, numerical model, parameterization, remote sensing, spatial resolution, temporal analysis, water budget, water resource},
	pages = {748--769}
}

