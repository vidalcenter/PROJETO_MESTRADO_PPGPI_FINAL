@article{rossetti_compactacao_2017,
	title = {Compactação em {Latossolos} e suas relações com o crescimento radicular do milho},
	volume = {11},
	copyright = {Direitos autorais 2017 REVISTA AGRO@MBIENTE ON-LINE},
	issn = {1982-8470},
	url = {https://revista.ufrr.br/agroambiente/article/view/3845},
	doi = {10.18227/1982-8470ragro.v11i3.3845},
	abstract = {A intensificação do tráfego de máquinas nas atividades agrícolas pode predispor o solo a uma condição de fragilidade, quando se considera o nível de tensão aplicada em cada sistema de manejo. Objetivou-se com este trabalho, avaliar sob diferentes estados de compactação em Latossolos a viabilidade de utilização de atributos físicos como indicadores de qualidade do solo e suas consequências sobre o sistema radicular do milho. O experimento foi conduzido no ano agrícola 2013/14, em delineamento de blocos casualizados em Latossolo Vermelho Distrófico (LVd) e Latossolo Vermelho Eutroférrico (LVef) em parcelas subdivididas, com quatro repetições. Os tratamentos foram: T0= preparo convencional, sem tráfego adicional; T1 e T2= duas e quatro passadas do trator de 4 t, respectivamente, T3 e T4= duas e quatro passadas do trator de 10 t, respectivamente. Foram coletadas amostras de solo nas camadas de 0-0,10 e 0,10-0,20 m, para determinação da densidade (Ds), porosidade, resistência do solo à penetração (RP) e parâmetros radiculares do milho. A RP elevou-se em 86\% para o LVd e LVef, em comparação do T0 ao T4. A redução da macroporosidade (Mac) em LVd de 0,19 para 0,15 m3 m-3 influenciou positivamente o comprimento radicular do milho em comparação a condição sem tráfego adicional. Os atributos RP, Ds, Mac, podem ser utilizados como identificadores de possíveis compactações em LVd e LVef.},
	language = {pt},
	number = {3},
	urldate = {2020-05-21},
	journal = {REVISTA AGRO@MBIENTE ON-LINE},
	author = {Rossetti, Karina de Vares and Centurion, José Frederico},
	month = jul,
	year = {2017},
	keywords = {Zea mays., atributos físicos, sistema radicular},
	pages = {181--190}
}

