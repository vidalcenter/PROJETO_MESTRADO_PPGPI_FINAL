@article{schwartz_les_2006,
	title = {Les valeurs de base de la personne : théorie, mesures et applications},
	volume = {Vol. 47},
	issn = {0035-2969},
	shorttitle = {Les valeurs de base de la personne},
	url = {https://www.cairn.info/revue-francaise-de-sociologie-1-2006-4-page-929.htm},
	abstract = {Le concept de valeur en sciences sociales a souffert d\&\#8217;une absence de consensus, que ce soit sur la d\&\#233;finition des valeurs, sur leur contenu ou sur la structure des relations qu\&\#8217;elles entretiennent les unes avec les autres\&\#160;; il a \&\#233;galement souffert de l\&\#8217;absence de m\&\#233;thodes fiables permettant de mesurer les valeurs. Pour valider une th\&\#233;orie destin\&\#233;e \&\#224; combler, du moins partiellement, cette lacune, cet article pr\&\#233;sente des donn\&\#233;es provenant de plus de soixante-dix pays et utilisant deux instruments de mesure diff\&\#233;rents. On traite ici des valeurs de base que les individus reconnaissent comme telles dans toutes les cultures. La th\&\#233;orie distingue dix valeurs de base diff\&\#233;rentes et d\&\#233;crit la dynamique des oppositions et des compatibilit\&\#233;s entre elles. Cette dynamique d\&\#233;termine une structure des relations entre valeurs, structure commune \&\#224; des groupes appartenant \&\#224; des cultures diff\&\#233;rentes, ce qui conduit \&\#224; penser qu\&\#8217;il existerait une organisation universelle des motivations humaines. Les individus, comme les groupes, se distinguent par les priorit\&\#233;s qu\&\#8217;ils donnent \&\#224; ces valeurs. On examine ici d\&\#8217;o\&\#249; proviennent les diff\&\#233;rences de priorit\&\#233; que les individus accordent aux valeurs, et les cons\&\#233;quences en termes d\&\#8217;attitudes et de comportements qui d\&\#233;coulent de ces diff\&\#233;rences de priorit\&\#233;s. On traite ainsi des processus qui influent sur les valeurs, et de ceux par lesquels elles influent sur l\&\#8217;action.},
	language = {fr},
	number = {4},
	urldate = {2020-04-13},
	journal = {Revue francaise de sociologie},
	author = {Schwartz, Shalom H.},
	year = {2006},
	pages = {929--968}
}

@article{fayolle_beyond_2014,
	title = {Beyond entrepreneurial intentions: values and motivations in entrepreneurship},
	volume = {10},
	issn = {1555-1938},
	shorttitle = {Beyond entrepreneurial intentions},
	url = {https://doi.org/10.1007/s11365-014-0306-7},
	doi = {10.1007/s11365-014-0306-7},
	abstract = {The role of values and motivations in understanding the entrepreneurial cognitive process deserves closer attention. So far, the predominant approach has been studying the entrepreneurial intention. This has been a very fruitful avenue of research. However, there is still much to be known to fully understand how the decision to start a venture is formed, and how the individual acts to implement that decision. Values and motivations are closely related, since both of them are linked to the notion of goals. The higher the importance attached to a goal, the more likely the person will form action plans to achieve it. In this paper, we present some ideas on how values and motivations could be integrated into the predominant entrepreneurial intention model. Firstly, we propose that values may help to explain the formation of intention antecedents (e.g., attitudes), and also moderate their effect on the entrepreneurial intention. Secondly, we point to the possibility that values and motivations may also play a role in the intention–action link. In particular, they may be instrumental in overcoming the barriers and obstacles toward effective start-up. In addition, this paper is also an introduction to the special issue, and we present the four papers selected to be included and highlight some of the most interesting aspects of their contributions.},
	language = {en},
	number = {4},
	urldate = {2020-04-13},
	journal = {International Entrepreneurship and Management Journal},
	author = {Fayolle, Alain and Liñán, Francisco and Moriano, Juan A.},
	month = dec,
	year = {2014},
	pages = {679--689}
}

