@article{simo_impact_2018,
	title = {Impact of the {Surface}–{Atmosphere} {Variables} on the {Relation} {Between} {Air} and {Land} {Surface} {Temperatures}},
	volume = {175},
	issn = {1420-9136},
	url = {https://doi.org/10.1007/s00024-018-1930-x},
	doi = {10.1007/s00024-018-1930-x},
	abstract = {AbstratoA energia, o momentum e as trocas de massa entre o solo e a atmosfera ocorrem na camada superficial. A diferença entre a temperatura da tela do ar a 2 m (T2) e a temperatura da superfície terrestre (LST) é um parâmetro chave na sua determinação. Aqui, uma análise dessa diferença é feita com dados de um site na ilha de Maiorca, no Mediterrâneo Ocidental. Enquanto estudos prévios são freqüentemente construídos para intervalos temporais relativamente curtos (de vários dias a poucos meses), o trabalho atual analisa séries de 2 anos de duração com uma resolução temporal de 30 minutos, em particular a variabilidade sazonal e diurna. A incerteza de LST medida perto do solo é estimada em pelo menos 2 ∘∘{\textasciicircum}\{{\textbackslash}circ \}C. A diferença entre T2 e LST (T2-LST) no centro do dia varia entre - 3 e - 10 ∘∘{\textasciicircum}\{{\textbackslash}circ \}C, chegando a cerca de - 18 ∘∘{\textasciicircum}\{{\textbackslash}circ \}C para os dias secos de verão. Estatisticamente, é mostrado que estes valores estão relacionados com o aquecimento da radiação da superfície transmitida para cima pela turbulência térmica. Os valores no verão são muito maiores porque o solo seco superior não permite que o calor seja transmitido eficientemente ao solo. À noite T2-LST é geralmente menos de 3 ∘∘{\textasciicircum}\{{\textbackslash}circ \}C, com correlações fracas de T2-LST com radiação líquida ou fluxo no solo, indicando que estes processos são ativos e eficientes no transporte de calor na maioria dos casos. As melhores quantidades correlacionadas à noite com T2-LST são a temperatura do solo superior, o LST e o fluxo de calor turbulento, refletindo a importância do estado da superfície. Nas noites de verão T2-LST leva valores muito pequenos, abaixo de 1 ∘∘{\textasciicircum}\{{\textbackslash}circ \}C, mesmo permitindo estratificação instável. Este é o resultado de um solo no qual os primeiros centímetros são muito quentes após o pôr do sol, não sendo capazes de enviar calor para o solo seco e liberá-lo na atmosfera.},
	language = {en},
	number = {11},
	urldate = {2018-05-23},
	journal = {Pure and Applied Geophysics},
	author = {Simó, Gemma and Martínez-Villagrasa, Daniel and Jiménez, Maria A. and Caselles, Vicente and Cuxart, Joan},
	month = nov,
	year = {2018},
	keywords = {-atmosfera, -onda radiação, Instrumentação de, calor do solo, fluxo de calor turbulento fluxo de, gradiente de temperatura, incertezas, interações superfície, temperatura da superfície terrestre},
	pages = {3939--3953}
}

