	keywords = {Average annual growth, China, Decomposition method, Decomposition methods, Driving forces, Industrial structures, Industrial water, Industrial water use, Innovation, Technological forecasting, Tianjin, Water - savings, Water conservation, Water use efficiency, Water-saving, Water-saving cities, decomposition analysis, industrial technology, nocv1, water resource, water use efficiency},
	pages = {53--61}
}

