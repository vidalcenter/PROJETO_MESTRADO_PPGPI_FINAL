	keywords = {Artificial waterspreading, Boundary conditions, Clogging, Ecological monitoring, Ecology, Forecasting, Groundwater, Groundwater pollution, Groundwater reservoir, Hydrochemical mode, Hydrochemicals, Hydrochemistry, Infiltration, Infiltration basin, Infiltration basins, Innovation, Mechanical purification, Mixtures, Physical model, Pollution, Pollution level, Purification, Reservoirs (water), River pollution, Rivers, Sanitary-epidemiological treatment, Silt, Surface treatment, Surface waters, Water pollution, Water resources, Water supply},
