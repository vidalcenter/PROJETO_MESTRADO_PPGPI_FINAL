}

@article{quintella_estudo_2019,
	title = {{ESTUDO} {PROSPECTIVO} {EXPLORATÓRIO} {DAS} {PATENTES} {DE} {MÉTODOS} {DE} {APRENDIZAGEM} {DE} {MÁQUINA} {APLICADOS} {AO} {MERCADO} {FINANCEIRO}},
