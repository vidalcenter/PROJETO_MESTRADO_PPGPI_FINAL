@article{mercadante_analysis_nodate,
	title = {{AN} {ANALYSIS} {OF} {PHARMACEUTICAL} {PATENT} {PROSECUTION} {IN} {BRAZIL} {AFTER} {TRIPS}: {TERM} {OF} {PATENT} {AND} {THEORETICAL} {EQUILIBRIUM}},
	abstract = {In 1996, the pharmaceutical products and processes became patentable once again in Brazil, as imposed by the Trade-Related Aspects of Intellectual Property Rights Agreement, even though the Brazilian Patent Office or INPI, as known in Brazil, still lacked the qualified personnel. Methodologically, we define patents as an indirect stimulus to innovation based on a contract between the patentee and society, where the former receives exclusivity in the commercial exploration in exchange for the public and sufficient disclosure of knowledge. The goal of this paper is to do an ex-post analysis of the prosecution of 293 granted pharmaceutical patents and test the hypothesis that the system potentially favors the patentee in detriment of society, due to the expansion of the term of patent. After the investigation of patentees, deadlines, prosecution, and documents, the results show, on one hand, a significant delay in the prosecution of these applications by the INPI, missing its deadlines, and, on the other hand, the patentees’ strategy to use almost entirely the time they have. Therefore, the combination of the patent prosecution and some legal provisions such as the minimum of term of patent tips the balance between patentee and society, in favor of the former.},
	language = {en},
	author = {Mercadante, Eduardo and Hasenclever, Lia and Paranhos, Julia},
	pages = {19}
}

