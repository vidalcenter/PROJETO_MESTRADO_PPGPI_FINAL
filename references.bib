	keywords = {Article, Attitude, Barriers to innovations, California, Collective action, Decision Making, Organizational, Decision makers, Decision making, Decision-making, Innovation, Innovation system, Managers, Organizational Innovation, Regulatory compliance, Risk, Risks, Surveys and Questionnaires, Technology, Underinvestment, United States, Urban wastewater, Urban water systems, Urbanization, Waste Water, Wastewater, Wastewater utilities, Water Purification, Water Resources, analysis, attitude, attitude to change, collective action, cost, decision making, financial management, human, innovation, investment, manager, organization, procedures, public attitude, questionnaire, risk perception, self concept, supply and distribution, technology, urban area, urbanization, utility sector, waste water, waste water management, wastewater, water management, water quality, water supply, water technology},
