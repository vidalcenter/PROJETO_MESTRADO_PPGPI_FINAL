	pmid = {28407547},
	keywords = {Biotransformation, Botrytis, Botrytis cinerea, Copper, Cu-oxalate, Culture Media, Detoxification mechanisms, Fungicides, Industrial, Metal oxide nanoparticles, Metal pollution, Nanoparticles, Oxides, Plants},
	pages = {178--185}
}

