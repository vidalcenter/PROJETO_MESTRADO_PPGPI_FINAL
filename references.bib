@article{ahn_green_2020,
	title = {Green hotel brands in {Malaysia}: perceived value, cost, anticipated emotion, and revisit intention},
	volume = {23},
	issn = {1368-3500},
	shorttitle = {Green hotel brands in {Malaysia}},
	url = {https://www-tandfonline.ez20.periodicos.capes.gov.br/doi/full/10.1080/13683500.2019.1646715},
	doi = {10.1080/13683500.2019.1646715},
	abstract = {Embora nos últimos anos, as práticas verdes tenham recebido considerável interesse na indústria do turismo e da hotelaria, faltam estudos que explorem os determinantes do comportamento positivo dos clientes. Assim, este estudo tem como objetivo entender o mecanismo de como os clientes para a lealdade atitudinal para com o hotel verde. Especificamente, com base no quadro cognitivo-afetivo-conative, o modelo proposto examina a ligação entre avaliação cognitiva (ou seja, custo e valor percebidos), respostas afetivas (ou seja, emoção e atitude antecipadas positivas/negativas) e sentido conativo (por exemplo, intenção comportamental). Os achados suportam o impacto relativo do valor percebido dos clientes e os benefícios sobre a emoção positiva e negativa antecipada. Além disso, os resultados também proporcionam uma compreensão de antecedentes importantes na criação de intenções comportamentais de atitude dos clientes em relação às marcas de hotéis verdes. Assim, este estudo preenche a lacuna de pesquisa examinando o mecanismo de formação da lealdade atitudinal dos clientes no ambiente do hotel verde.},
	number = {12},
	urldate = {2020-09-18},
	journal = {Current Issues in Tourism},
	author = {Ahn, Jiseon and Kwon, Jookyung},
	month = jun,
	year = {2020},
	note = {Publisher: Routledge},
	pages = {1559--1574}
}

@article{lopez_what_2019,
	title = {What makes (im)politeness for travellers? {Spanish} tourists’ perceptions at national and international hotels},
	volume = {15},
	issn = {1613-4877, 1612-5681},
	shorttitle = {What makes (im)politeness for travellers?},
	url = {https://www-degruyter-com.ez20.periodicos.capes.gov.br/view/journals/jplr/15/2/article-p195.xml},
	doi = {10.1515/pr-2016-0060},
	abstract = {{\textless}section class="abstract"{\textgreater}{\textless}h2 class="abstractTitle text-title my-1" id="d1050e2"{\textgreater}Abstract{\textless}/h2{\textgreater}{\textless}p{\textgreater}Although hotel service encounters have been widely studied, the literature presents two main shortcomings: 1) receptionist-guest communication has not received much attention from a politeness perspective; and 2) the scarcity of studies regarding politeness1 (i. e., the interlocutors’ perception of politeness) has been overshadowed by the vast amount of literature concerning politeness2. Regarding customer perception, understanding what appropriate communicative behaviour is - i. e. whether (non)verbal actions are conducive to variation in terms of customer perception and satisfaction - is crucial to understand service success or failure. In this context, our study examines the opinion and perception of 183 Spanish participants who had stayed at national and international hotels and who assessed, on the one hand, to what extent the receptionists were polite or impolite, and why; and on the other, how communication with customers could improve in terms of politeness. The findings indicate that, despite the participants’ general preference for friendliness and solidarity, the politeness strategies that participants valued as adequate in this context are not necessarily those traditionally related to Peninsular Spanish (i. e. directness and/or involvement). Also, this study shows that what lay people consider politeness encompasses a great number of variables in which linguistic production is but one of them.{\textless}/p{\textgreater}{\textless}/section{\textgreater}},
	language = {en},
	number = {2},
	urldate = {2020-09-18},
	journal = {Journal of Politeness Research},
	author = {López, María de la O. Hernández and Amaya, Lucía Fernández},
	month = jul,
	year = {2019},
	note = {Publisher: De Gruyter Mouton
Section: Journal of Politeness Research},
	pages = {195--222}
}

