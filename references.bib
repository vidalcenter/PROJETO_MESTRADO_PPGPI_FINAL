	author = {Kanazawa, Flávio Notomi and Giraldi, Janaina de Moura Engracia and Oliveira, Jorge Henrique Caldeira de and Kanazawa, Flávio Notomi and Giraldi, Janaina de Moura Engracia and Oliveira, Jorge Henrique Caldeira de},
