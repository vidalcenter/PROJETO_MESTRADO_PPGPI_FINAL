@article{pandit_nutrient_2020,
	title = {Nutrient effect of various composting methods with and without biochar on soil fertility and maize growth},
	volume = {66},
	issn = {0365-0340},
	url = {https://www-tandfonline.ez20.periodicos.capes.gov.br/doi/full/10.1080/03650340.2019.1610168},
	doi = {10.1080/03650340.2019.1610168},
	abstract = {Este trabalho mostrou pela primeira vez que técnicas de transformação de nutrientes orgânicos baseados em materiais disponíveis localmente (esterco, resíduos verdes e biochar avançado) podem aumentar a eficiência de fertilização do substrato resultante por um fator de três em comparação com outras emendas orgânicas sem biocarvão. Utilizamos três métodos diferentes de compostagem para investigar as técnicas de transformação de nutrientes orgânicos; i) compostagem convencional (processo de compostagem concluído sem virar as pilhas) ii) compostagem aeróbica (processo de compostagem com rotação manual de pilhas) e iii) compostagem bokashi (fermentação anaeróbica por lacto). A compostagem foi realizada na ausência (composto somente) e na presença de biochar (co-composto). O biocarvão foi produzido localmente a partir de um arbusto invasor ' Eupatorium adenophorum' Um ensaio em vaso com milho cultivado em solo argiloso foi realizado para investigar o efeito agronômico produzido usando três métodos de compostagem mencionados acima, que foram comparados com fertilizantes minerais convencionais (NPK). Efeitos significativos do bokashi-biochar co-composto (60 t ha- 1 ) foram observados no crescimento do milho, que aumentou a biomassa em 243\% em comparação com a NPK mineral, mostrando também melhores efeitos de crescimento do que as emendas de composições aeróbicas e convencionais. Os nutrientes disponíveis no solo melhorados (P disponível e outros cátions base trocáveis ​​(K + , Ca 2+ e Mg 2+ )) foram provavelmente a causa do efeito de crescimento superior do bokashi-biochar co-composto.},
	number = {2},
	urldate = {2020-04-15},
	journal = {Archives of Agronomy and Soil Science},
	author = {Pandit, Naba Raj and Schmidt, Hans Peter and Mulder, Jan and Hale, Sarah E and Husson, Olivier and Cornelissen, Gerard},
	month = jan,
	year = {2020},
	pages = {250--265}
}

