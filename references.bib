	issn = {2525-8761},
	url = {https://www.brazilianjournals.com/index.php/BRJD/article/view/9419},
	doi = {10.34117/bjdv6n4-431},
	abstract = {Historicamente Manaus apresenta carência de material pétreo, solos superficiais inadequados à pavimentação, bem como altos custos ocasionados pelas grandes distâncias para aquisição de insumos de melhor qualidade. Desta forma estudou-se o efeito do acréscimo de resíduo de louça sanitária moído a um solo argiloso típico da cidade de Manaus como alternativa de estabilização granulométrica para melhorar as propriedades geotécnicas. Foram analisadas as seguintes composições: 100\% de solo natural, 95\% de solo natural + 5\% de aditivo, 90\% de solo natural + 10\% de aditivo. O solo natural foi analisado segundo os ensaios de massa específica dos grãos, granulometria, limites de plasticidade e liquidez, ensaio de Proctor na energia intermediária e resistência ao cisalhamento direto. O resíduo de louça sanitária foi caracterizado segundo sua granulometria. As composições foram avaliadas segundos os ensaios de limite de liquidez e plasticidade, Proctor na energia intermediária e resistência ao cisalhamento direto. Os resultados apontaram que a adição do material alternativo ao solo argiloso:  diminuiu o índice de plasticidade, diminuiu a umidade ótima, aumentou a massa específica aparente seca, aumentou o ângulo de atrito e diminuiu a coesão do solo. As composições com material alternativo apresentaram maior resistência ao cisalhamento quando comparadas com o material natural, sugerindo uma destinação técnica e ambiental a este resíduo.},
	language = {pt},
	number = {4},
	urldate = {2020-07-23},
	journal = {Brazilian Journal of Development},
	author = {Rocha, Licia Liz Bastos Garcês da and Silva, Paulo Sérgio da and Antunes, Rebecca Lima and Silva, Matheus Pena da Silva e and Frota, Consuelo Alves da},
	month = apr,
	year = {2020},
	note = {Number: 4},
	keywords = {Cisalhamento Direto, Coesão Ângulo de Atrito, Resíduo Cerâmico, Solo},
	pages = {22617--22627}
}

